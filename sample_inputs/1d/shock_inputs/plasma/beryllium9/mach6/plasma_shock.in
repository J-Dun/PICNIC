#####################################################
grid.num_cells     = 1792
grid.is_periodic   = 0
grid.config_decomp = 224
grid.geometry      = "cartesian"
grid.num_ghosts    = 3

grid.X_min = 0.0
grid.X_max = 3200.0 # [mfpi]
grid.verbosity = 1

